{
    "manifest_version": 2,
    "name": "SecUSB",
    "version": "1.0",
    "description": "SecUSB Chrome Extension",
    "permissions" : ["tabs", "activeTab"],
    "background": {
        "scripts" : ["background.js"]
    },
    "content_scripts":[{
        "matches": ["*://*.facebook.com/*"],
        "js": ["content.js"]
    }]
}